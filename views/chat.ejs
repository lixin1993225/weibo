<!DOCTYPE html>
<html>
<head>
	<title>chat</title>
	<link rel="stylesheet" href="./css/bootstrap.min.css">
	<link rel="stylesheet" href="./css/bootstrap-theme.min.css">	
	<script src="./js/jquery.min.js"></script>
	<script src="./js/bootstrap.min.js"></script>
	<script src="./js/jquery.Jcrop.min.js"></script>
	<script type="text/javascript" src="./js/socket.io.js"></script>
</head>
<body>
	<!-- header -->
	<%- include('./header.ejs',{"where":"sigin"})%>
	<div class="container">
		<div class="chatbox jumbotron">
			<h1>1123</h1>
			<div class="row">
			  	<div class="col-lg-12">
			    	<div class="input-group">
			      		<input type="text" class="form-control" placeholder="Search for..." style="height:36px">
			      		<span class="input-group-btn">
			        		<button class="btn btn-default textinn" type="button">Go!</button>
			      		</span>
			    	</div><!-- /input-group -->
			  	</div><!-- /.col-lg-6 -->
			</div><!-- /.row -->
		</div>
	</div>
	<script type="text/javascript">
		let sock = io.connect("ws://localhost:8089");
		$(".textinn").click(function(){
			sock.emit("textInn",$(".form-control").val())
		});
	</script>
</body>
</html>